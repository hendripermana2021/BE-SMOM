{
  "name": "be-smom",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create": "npx sequelize db:create",
    "dev": "nodemon ./index.js",
    "start": "node ./index.js",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo": "npx sequelize-cli db:migrate:undo:all",
    "db:drop": "npx sequelize-cli db:drop",
    "db:restart": "npx sequelize-cli db:drop db:create db:migrate db:seed",
    "tbl_guru": "npx sequelize-cli model:generate --name tbl_guru --attributes name_guru:string,sex:string,bid_pendidikan:string,pendidikan:string,email:string,password:string,real_password:string,role_id:string,accesstoken:text,refreshtoken:text",
    "tbl_siswa": "npx sequelize-cli model:generate --name tbl_siswa --attributes name_siswa:string,sex:string,fathername:string,mothername:string,status:string,id_class:integer,email:string,password:string,real_password:string,role_id:string,image:string,accesstoken:text,refreshtoken:text",
    "tbl_class": "npx sequelize-cli model:generate --name tbl_class --attributes name_class:string,id_walkes:integer",
    "tbl_modul": "npx sequelize-cli model:generate --name tbl_modul --attributes id_guru:integer,title:string,content:text,status_post:string,image:string",
    "tbl_reference": "npx sequelize-cli model:generate --name tbl_reference --attributes id_modul:integer,id_content:string,link:string,source:string",
    "tbl_modul_assignment": "npx sequelize-cli model:generate --name tbl_modul_assignment --attributes id_modul:integer,id_class:integer",
    "tbl_readviews": "npx sequelize-cli model:generate --name tbl_readviews --attributes id_modul:integer,id_siswa:integer,status:string",
    "tbl_contentpost": "npx sequelize-cli model:generate --name tbl_contentpost --attributes id_guru:integer,content:text,views:integer",
    "seed-guru": "npx sequelize-cli seed:generate --name demo-tbl_guru",
    "seed-siswa": "npx sequelize-cli seed:generate --name demo-tbl_siswa",
    "seed-class": "npx sequelize-cli seed:generate --name demo-tbl_class",
    "seed-modul": "npx sequelize-cli seed:generate --name demo-tbl_modul",
    "seed-reference": "npx sequelize-cli seed:generate --name demo-tbl_reference",
    "seed-status_homework": "npx sequelize-cli seed:generate --name demo-tbl_status_homework",
    "seed-readviews": "npx sequelize-cli seed:generate --name demo-tbl_readviews",
    "seed-contentpost": "npx sequelize-cli seed:generate --name demo-tbl_contentpost"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "babel-loader": "^9.1.3",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.10.0",
    "nodemon": "^3.1.3",
    "path": "^0.12.7",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2"
  }
}
